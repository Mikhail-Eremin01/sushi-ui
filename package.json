    "axios": "^1.6.8",
    "env-cmd": "^10.1.0",
    "graphql": "^16.5.0",
    "install": "^0.13.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.3",
    "moment-range": "^4.0.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
